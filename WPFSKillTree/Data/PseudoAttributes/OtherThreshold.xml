		<Attribute Name="+# Str Based Jewel" ConversionMultiplier="-7.0" />
	</PseudoAttribute>
	<PseudoAttribute Name="#% increased Hybrid Life/ES with Life Jewels Minus Int Threshold" Group="Defense">
