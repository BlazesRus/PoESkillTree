<?xml version="1.0" encoding="utf-8"?>
<PseudoAttributes>
	<!-- Life Jewels = 7% Life Jewel, Max Rolled Rare ES Jewels = 8% Energy Shield Jewels-->
	<PseudoAttribute Name="#% increased Hybrid Life/ES Percent and  Life Jewels" Group="Defense">
		<Attribute Name="#% increased maximum Life" />
		<Attribute Name="#% increased maximum Energy Shield" />
		<Attribute Name="+# to Intelligence" ConversionMultiplier="0.2" />
		<Attribute Name="+# Jewel Socket" ConversionMultiplier="7.0" />
	</PseudoAttribute>
	<PseudoAttribute Name="#% increased Life with Seven Percent Life Jewels" Group="Defense">
		<Attribute Name="#% increased maximum Life" />
		<Attribute Name="+# Jewel Socket" ConversionMultiplier="7.0" />
	</PseudoAttribute>
	<PseudoAttribute Name="#% increased Life with Life Jewels Minus Int Threshold" Group="Defense">
		<Attribute Name="#% increased maximum Life" />
		<Attribute Name="+# Jewel Socket" ConversionMultiplier="7.0" />
		<Attribute Name="+# Int Based Jewel" ConversionMultiplier="-7.0" />
	</PseudoAttribute>
	<PseudoAttribute Name="#% increased Life with Life Jewels Minus Dex Threshold" Group="Defense">
		<Attribute Name="#% increased maximum Life" />
		<Attribute Name="+# Jewel Socket" ConversionMultiplier="7.0" />
		<Attribute Name="+# Dex Based Jewel" ConversionMultiplier="-7.0" />
	</PseudoAttribute>
